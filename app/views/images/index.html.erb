<% for image in @images do %>
  <h2><%= image.id %>) <%=h image.title %></h2>
  <image src=<%= image.url %> width=300 alt="<%=h image.title %>">
  <ul>
  <li>Artist: <%= image.artist %></li>
  <li>Gallery: <%= image.gallery %></li>
  <li>Organization: <%= image.organization %></li>
  <li>Media: <%= image.media %> (<%= image.width %>x<%= image.height %>)</li>
  </ul>
<% end %>

